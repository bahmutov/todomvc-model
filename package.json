{
  "name": "todomvc-model",
  "version": "1.0.0",
  "description": "Model object for TodoMVC apps",
  "main": "src/todos.js",
  "scripts": {
    "test": "node --harmony node_modules/.bin/rocha src/*-spec.js",
    "lint": "standard *.js",
    "commit": "commit-wizard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/todomvc-model.git"
  },
  "keywords": [
    "todo",
    "todos",
    "todomvc",
    "model"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/todomvc-model/issues"
  },
  "homepage": "https://github.com/bahmutov/todomvc-model#readme",
  "config": {
    "pre-git": {
      "commit-msg": [
        "simple"
      ],
      "pre-commit": [
        "npm run lint",
        "npm run test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "devDependencies": {
    "pre-git": "3.1.2",
    "rocha": "1.6.1",
    "standard": "5.4.1"
  },
  "dependencies": {
    "check-more-types": "2.10.0",
    "lazy-ass": "1.3.0"
  }
}
